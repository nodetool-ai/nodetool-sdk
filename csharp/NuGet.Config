<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <packageSources>
    <clear />
    <add key="nuget.org" value="https://api.nuget.org/v3/index.json" />
    <add key="Stride" value="https://packages.stride3d.net/nuget" />
    <add key="vvvv-public-feed" value="https://teamcity.vvvv.org/guestAuth/app/nuget/v1/FeedService.svc/" />
  </packageSources>

  <packageSourceMapping>
    <packageSource key="nuget.org">
      <package pattern="*" />
    </packageSource>
    <packageSource key="Stride">
      <package pattern="Stride.*" />
    </packageSource>
    <packageSource key="vvvv-public-feed">
      <package pattern="VL.*" />
      <package pattern="VVVV.*" />
    </packageSource>
  </packageSourceMapping>
</configuration>
